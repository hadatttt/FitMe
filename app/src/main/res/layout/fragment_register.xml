<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".register.RegisterFragment">

    <ScrollView
        android:id="@+id/ra21204di08e"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#FFFFFF"
        android:clipChildren="false"
        android:clipToPadding="false"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/r908lqpgw4ru"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="14dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical">
                    <RelativeLayout
                        android:id="@+id/r73a6efvkmcq_rl"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <ImageView
                            android:id="@+id/r73a6efvkmcq"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:src="@drawable/r73a6efvkmcq"
                            android:scaleType="fitXY"
                            />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:paddingBottom="55dp">

                            <LinearLayout
                                android:id="@+id/r5tcdh50zou8"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="78dp"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:paddingHorizontal="21dp"
                                android:paddingVertical="13dp">

                                <LinearLayout
                                    android:id="@+id/r4bhq2y007w8"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:orientation="vertical"></LinearLayout>
                            </LinearLayout>

                            <TextView
                                android:id="@+id/r5a53qjilsc"
                                android:layout_width="195dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30dp"
                                android:layout_marginBottom="54dp"
                                android:text="Create \nAccount"
                                android:textColor="#202020"
                                android:textSize="50sp"
                                android:textStyle="bold" />

                            <ImageView
                                android:id="@+id/r64k2r9nk10u"
                                android:layout_width="90dp"
                                android:layout_height="90dp"
                                android:layout_marginStart="30dp"
                                android:layout_marginBottom="32dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/upload_photo" />

                            <EditText
                                android:id="@+id/r0yzi0lzr3tlh"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="20dp"
                                android:background="@drawable/cr59bf8f8f8"
                                android:hint="Email"
                                android:inputType="text"
                                android:paddingVertical="16dp"
                                android:paddingLeft="19dp"
                                android:paddingRight="38dp"
                                android:textColor="#D2D2D2"
                                android:textSize="14sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rkrti44kffs8"
                    android:layout_marginBottom="-6dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginStart="20dp"
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="@drawable/cr59bf8f8f8"
                    android:paddingTop="15dp"
                    android:paddingBottom="17dp"
                    android:paddingHorizontal="19dp"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/ru8ulkjqgcn"
                        android:textColor="#D2D2D2"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="Password"
                        />
                    <ImageView
                        android:id="@+id/r1qglcvui59o"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/eye_slash"
                        app:shapeAppearanceOverlay="@style/cr59" />
                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/phoneInputLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:background="@drawable/cr59bf8f8f8"
                android:paddingVertical="16dp"
                android:paddingHorizontal="19dp"
                android:layout_marginBottom="52dp"
                android:layout_marginHorizontal="20dp">

                <!-- Country Code Picker: có cờ, mã quốc gia, mũi tên xổ xuống -->
                <com.hbb20.CountryCodePicker
                    android:id="@+id/countryCodePicker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:ccpDialog_showTitle="true"
                    app:ccpDialog_showFlag="true"
                    app:ccpDialog_showNameCode="true"
                    app:ccpDialog_showPhoneCode="true"/>


                <!-- Divider -->
                <View
                    android:layout_width="1dp"
                    android:layout_height="23dp"
                    android:background="#1E1E1E"
                    android:layout_marginEnd="13dp" />

                <!-- Ô nhập số điện thoại -->
                <EditText
                    android:id="@+id/etPhoneNumber"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:hint="Your number"
                    android:textColor="#D2D2D2"
                    android:textSize="14sp"
                    android:inputType="phone"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rbgl7d5rbxk9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/bg_button"
                android:paddingVertical="17dp"
                android:layout_marginBottom="24dp"
                android:layout_marginHorizontal="20dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rv07pcvqs6d"
                    android:textColor="#F3F3F3"
                    android:textSize="22sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Done"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r5okzo9mt0xf"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="53dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_cancel"
                    android:textColor="#202020"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Cancel"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rnvtwk9v3l7i"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="9dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical">
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
